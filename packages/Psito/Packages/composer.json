{
    "name": "psito/packages",
    "description": ":package_description",
    "keywords": [
        "Packages",
        "Psito"
    ],
    "homepage": "https://github.com/psito/packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Culinary Street",
            "email": "dev@psito.org"
        }
    ],
    "require": {
        "illuminate/support": "5.1.*",
        "illuminate/console": "5.1.*",
        "illuminate/filesystem": "5.1.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Psito\\Packages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psito\\Packages\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
