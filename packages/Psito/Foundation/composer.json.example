{
    "name": "psito/foundation",
    "description": ":package_description",
    "keywords": [
        "culinarystreet",
        "psito",
        "platform",
        "framework"
    ],
    "homepage": "https://github.com/psito/psito",
    "license": "MIT",
    "authors": [
             {
                 "name": "Culinary Street",
                 "email": "dev@psito.org"
             }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Psito\\Foundation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psito\\Foundation\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
