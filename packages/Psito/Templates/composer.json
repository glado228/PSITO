{
    "name": "psito/templates",
    "description": ":package_description",
    "keywords": [
        "league",
        ":package_name"
    ],
    "homepage": "https://github.com/psito/templates",
    "license": "MIT",
    "authors": [
        {
            "name": "Culinary Street",
            "email": "dev@psito.org"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*",
        "illuminate/view": "5.1.*",
        "twig/twig": "~1.18"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Psito\\Templates\\": "packages/Psito/Templates/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psito\\Templates\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "psito/templates": "Psito Platform Twig integration package"
    }
}
