{
    "name": "psito/workbench",
    "description": ":package_description",
    "keywords": [
        "psito",
        ":package_name"
    ],
    "homepage": "https://github.com/psito/workbench",
    "license": "MIT",
    "authors": [
        {
            "name": "Culinary Street",
            "email": "dev@psito.org"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/filesystem": "5.1.*",
        "illuminate/support": "5.1.*",
        "symfony/finder": "2.7.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "illuminate/console": "5.1.*"
    },
    "autoload": {
        "psr-4": {
            "Psito\\Workbench\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psito\\Workbench\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
